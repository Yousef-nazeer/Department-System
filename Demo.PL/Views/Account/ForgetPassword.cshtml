
@{
    ViewData["Title"] = "ForgetPassword";
    Layout = "~/Views/Shared/_AuthLayout.cshtml";
}
@model ForgetPasswordViewModel
<div class="main-agileinfo">
	<div class="agileits-top">
		<form method="post" asp-action="SendEmail">
			<div asp-validation-summary="All"></div>
			<input class="text" asp-for="Email" placeholder="Email" required="">
			<br />
		
			<input type="submit" value="Send Email">
		</form>
		<p>Don't you have an Account? <a asp-action="Register"> Register  Now!</a></p>
		<p>Forger Password? <a asp-action="ForgetPassword"> Forger Password>?</a></p>
	</div>
</div>

